{
  "mcpServers": {
    "codegraph": {
      "command": "codegraph",
      "args": [
        "start",
        "stdio"
      ]
    },
    "serena": {
      "command": "node",
      "args": [
        "mcp/mcp-docker-runner.js",
        "serena"
      ]
    },
    "codanna": {
      "command": "node",
      "args": [
        "mcp/mcp-docker-runner.js",
        "codanna"
      ]
    },
    "shrimp-task-manager": {
      "command": "node",
      "args": [
        "mcp/mcp-docker-runner.js",
        "shrimp"
      ]
    },
    "context7": {
      "command": "node",
      "args": [
        "mcp/mcp-docker-runner.js",
        "context7"
      ],
      "env": {
        "CONTEXT7_API_KEY": "${CONTEXT7_API_KEY}"
      }
    }
  }
}
