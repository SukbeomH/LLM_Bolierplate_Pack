{
  "mcpServers": {
    "graph-code": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@er77/code-graph-rag-mcp", "."],
      "env": {
        "MCP_TIMEOUT": "80000",
        "NODE_OPTIONS": "--max-old-space-size=4096"
      }
    },
    "memorygraph": {
      "type": "stdio",
      "command": "memorygraph",
      "args": ["--profile", "extended", "--backend", "falkordblite"]
    },
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "${CONTEXT7_API_KEY}"
      }
    }
  },
  "enable_tool_search": true
}
